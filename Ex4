
package Cachorro;

public class Cachorro {
    int idade;
    String nome;
    
     void latir() {
         System.out.println("Latindo");
     }
     
     void uivar() {
         System.out.println("Uivando");
     }
     
     void deitar() {
         System.out.println("Deitando");
     }
     
     void rolar() {
         System.out.println("Rolando");
     }
}

