
package ex1;

public static void main(String[] args) {
        Retangulo retangulo = new Retangulo();
        
    }
}

public class Retangulo {
    double altura;
    double largura;
    double area;
    double perimetro;
    
    double calcularArea() {
        area = altura * largura;
        return area;
    }
    
    double calcularPerimetro() {
        perimetro = altura * largura;
        return perimetro;
    }
    
}
