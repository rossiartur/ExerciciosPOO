
package Lampada;

public class Lampada {
    String marca;
    String modelo;
    int potencia;
    boolean estadoLampada;
    
    void ligar() {
        System.out.println("Ligando");
    }
    
    void desligar() {
        System.out.println("Desligando");
    }
    
    void imprimir() {
        System.out.println("Imprimindo");
    }
    
}
